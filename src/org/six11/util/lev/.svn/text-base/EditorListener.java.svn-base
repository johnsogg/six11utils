package org.six11.util.lev;

/**
 * An interface for something to implement when they are interested in
 * knowing that an editor of some sort has changed its data. This is
 * most likely to be used in conjunction with anonymous modules, which
 * are used for editing data objects.
 */
public interface EditorListener {
  public void thingEdited(Object source);
}
