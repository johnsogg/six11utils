package org.six11.util.pen;

import java.util.EventObject;

/**
 * 
 *
 * @author Gabe Johnson <johnsogg@cmu.edu>
 */
public class OliveSoupEvent extends EventObject {

  protected String type;
  protected Object data;
  
  /**
   * @param source
   */
  public OliveSoupEvent(Object source, String type, Object data) {
    super(source);
    this.type = type;
    this.data = data;
  }

  public String getType() {
    return type;
  }

  public Object getData() {
    return data;
  }

}
